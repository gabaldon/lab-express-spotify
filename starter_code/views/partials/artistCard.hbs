<div class= "col-md-4">
    <article class="card">
        {{#if images.0.url}}
        <img class="card-img-top" src="{{images.0.url}}" alt="Card image">
        {{else}}
        <img class="card-img-top" src="images/cuna-de-queso-curado-viejo-la-rebonita-750gr.jpg" alt="Card image">
        {{/if}}
        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
    
    </article>
</div>